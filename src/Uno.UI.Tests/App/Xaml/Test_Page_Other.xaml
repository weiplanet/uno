<Page x:Class="Uno.UI.Tests.App.Xaml.Test_Page_Other"
	  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	  xmlns:win="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	  xmlns:not_win="http://uno.ui/not_win"
	  xmlns:local="using:Uno.UI.Tests.App.Xaml"
	  xmlns:views="using:Uno.UI.Tests.App.Views"
	  xmlns:ext="using:Uno.UI.Tests.ViewLibrary"
	  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	  mc:Ignorable="d not_win"
	  Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

	<Grid>
		<Grid x:Name="testGrid1" x:FieldModifier="public">
			<Grid.Resources>
				<local:Subclassed_Dictionary />
			</Grid.Resources>
			<Border x:Name="testBorder1"
					x:FieldModifier="public"
					Background="{StaticResource PerilousColorBrush}" />
		</Grid>
		<Grid>
			<Grid.Resources>
				<local:Subclassed_Dictionary />
			</Grid.Resources>
			<Border x:Name="testBorder2"
					x:FieldModifier="public"
					Background="{StaticResource PerilousColorBrush}" />
		</Grid>
		<Grid x:Name="customResourceResourcesGrid" x:FieldModifier="public">
			<Grid.Resources>
				<ResourceDictionary Source="{CustomResource OverlyConvolutedDictionaryLookup}" />
			</Grid.Resources>
		</Grid>
		<TextBlock x:Name="customResourceTextBlock" x:FieldModifier="public" Text="{CustomResource TapestryHereOnMySkin}" />
		<TextBlock x:Name="ResourceKeyThemedTextBlock"
				   x:FieldModifier="public"
				   Foreground="{ThemeResource ButtonForeground}" />
		<Grid>
			<Grid.Resources>
				<ResourceDictionary Source="ms-appx:///UnO.uI.TESTs.ViEWLiBrAry/THEmeS/moRE/TEsT_dIcTionaRy_AbS.xAml" />
			</Grid.Resources>
			<TextBlock x:Name="CaseInsensitiveSourceTextBlock"
					   x:FieldModifier="public"
					   Foreground="{StaticResource BituminousColorBrush}" />
		</Grid>
		<Border x:Name="SpaceInKeyBorder"
				x:FieldModifier="public"
				Background="{StaticResource Resource With Space In Key}" />
		<Border>
			<Border.Resources>
				<ResourceDictionary>
					<ResourceDictionary.ThemeDictionaries>
						<ResourceDictionary x:Key="Default">
							<SolidColorBrush x:Key="KeyInDictionaryWithOnlyThemeDictionaries"
											 Color="MediumPurple" />
						</ResourceDictionary>
					</ResourceDictionary.ThemeDictionaries>
				</ResourceDictionary>
			</Border.Resources>
			<TextBlock x:Name="ThemeDictionaryOnlyTextBlock"
					   x:FieldModifier="public"
					   Text="Warfarin' terrapin"
					   Foreground="{StaticResource KeyInDictionaryWithOnlyThemeDictionaries}" />
		</Border>
	</Grid>
</Page>

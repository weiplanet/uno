<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:win="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:local="using:Uno.UI.Tests.App.Views">
	<ResourceDictionary.ThemeDictionaries>
		<ResourceDictionary x:Key="Light">
			<Color x:Key="UnremarkableColor">LightBlue</Color>
		</ResourceDictionary>
		<ResourceDictionary x:Key="Dark">
			<Color x:Key="UnremarkableColor">DarkBlue</Color>
		</ResourceDictionary>
	</ResourceDictionary.ThemeDictionaries>
	<SolidColorBrush x:Key="UnremarkableColorBrush"
					 Color="{ThemeResource UnremarkableColor}" />
</ResourceDictionary>

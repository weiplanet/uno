<UserControl
    x:Class="UITests.Shared.Windows_UI_Xaml_Controls.ContentDialogTests.ContentDialog_Simple"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:UITests.Shared.Windows_UI_Xaml_Controls.ContentDialogTests"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

	<StackPanel>
	  <Button x:Name="showDialog1" Click="OnMyButtonClick" Content="Show Dialog" />
	  <Button x:Name="showDialog2" Click="OnMyButtonClick2" Content="Show Dialog 2" />
	  <Button x:Name="showDialog3" Click="OnMyButtonClick3" Content="Show Dialog 3" />
	  <Button x:Name="showDialog4" Click="OnMyButtonClick4" Content="Show Dialog 4 (Command)" />
	  <Button x:Name="showDialog5" Click="OnMyButtonClick5" Content="Show Dialog 5 (Primary as Default Button)" />
	  <Button x:Name="showDialog6" Click="OnMyButtonClick6" Content="Show Dialog 6 (Dialog Reuse)" />
	  <TextBlock x:Name="dialogResult" Text="Undefined" />
	  <TextBlock x:Name="commandResult" Text="Undefined" />
	</StackPanel>
</UserControl>

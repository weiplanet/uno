<Page
    x:Class="UITests.Windows_UI_Xaml_Controls.ImageTests.ImageSourceUrlMsAppDataScheme"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:UITests.Windows_UI_Xaml_Controls.ImageTests"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}"
    mc:Ignorable="d">

    <StackPanel Padding="12">
        <TextBlock Text="{Binding AppDataUri}" />
        <Image
            Width="100"
            Height="100"
            HorizontalAlignment="Left"
            VerticalAlignment="Top"
            Source="{Binding AppDataUri}" />
    </StackPanel>
</Page>

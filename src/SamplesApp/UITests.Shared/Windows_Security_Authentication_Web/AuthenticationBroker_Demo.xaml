<Page
	x:Class="SamplesApp.UITests.Windows_Security_Authentication_Web.AuthenticationBroker_Demo"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:local="using:SamplesApp.UITests.Windows_Security_Authentication_Web"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

	<StackPanel Spacing="6">
		<StackPanel Orientation="Horizontal">
			<Button Click="SignIn_Clicked" x:Name="btnSignin" IsEnabled="False">Sign In</Button>
			<Button Click="SignOut_Clicked" x:Name="btnSignout" IsEnabled="False">Sign Out</Button>
		</StackPanel>
		<TextBlock x:Name="resultTxt" />
	</StackPanel>
</Page>

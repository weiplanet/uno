<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:local="using:Uno.UI.Tests.ViewLibrary">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="ms-appx:///Uno.UI.Tests.ViewLibrary/Themes/MyExtControl.xaml" />
		<ResourceDictionary Source="ms-appx:///Uno.UI.Tests.ViewLibrary/Themes/More/Test_Dictionary_Merged.xaml" />
	</ResourceDictionary.MergedDictionaries>

	<SolidColorBrush x:Key="IrritableColorBrush"
					 Color="{StaticResource IrritableColor}" />

	<Style TargetType="local:MyTextBox">
		<Setter Property="Foreground"
				Value="{StaticResource IrritableColorBrush}" />
		<Setter Property="Background"
				Value="{StaticResource UnflappableColorBrush}" />
	</Style>
</ResourceDictionary>
